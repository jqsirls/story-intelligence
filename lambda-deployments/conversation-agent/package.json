{
  "name": "@storytailor/conversation-agent",
  "version": "1.0.0",
  "description": "Real-time conversational storytelling using ElevenLabs Agents Platform",
  "main": "dist/lambda.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "npm run build && zip -r conversation-agent.zip dist/ node_modules/ package.json"
  },
  "dependencies": {
    "@supabase/auth-js": "^2.77.0",
    "@supabase/functions-js": "^2.77.0",
    "@supabase/postgrest-js": "^2.77.0",
    "@supabase/realtime-js": "^2.77.0",
    "@supabase/storage-js": "^2.77.0",
    "@supabase/supabase-js": "^2.77.0",
    "aws-sdk": "^2.1692.0",
    "ioredis": "^5.8.2",
    "tslib": "^2.8.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "types": "./dist/lambda.d.ts"
}
