{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  },
  "references": [
    { "path": "./packages/shared-types" },
    { "path": "./packages/auth-agent" },
    { "path": "./packages/router" },
    { "path": "./packages/storytailor-agent" },
    { "path": "./packages/content-agent" },
    { "path": "./packages/library-agent" },
    { "path": "./packages/emotion-agent" },
    { "path": "./packages/commerce-agent" },
    { "path": "./packages/insights-agent" }
  ],
  "files": []
}